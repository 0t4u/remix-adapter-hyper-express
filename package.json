  "peerDependencies": {
    "@remix-run/node": "1.x",
    "hyper-express": "6.x"
  },
  "devDependencies": {
    "@augu/eslint-config": "^4.0.1",
    "@remix-run/node": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "hyper-express": "^6.7.0",
